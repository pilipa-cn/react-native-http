{
  "name": "react-native-http",
  "version": "1.1.9",
  "description": "react native http library",
  "main": "index.js",
  "scripts": {
    "test": "jest --json --outputFile=testsReport/jest.json"
  },

  "dependencies": {
    "moment": "~>2.21.0",
    "debounce": "^1.0.2",
    "json-stringify-safe": "^5.0.1",
    "react-native-invertible-scroll-view": "^1.1.0"
  },

  "devDependencies": {
    "react": "16.0.0-alpha.12",
    "react-native": "^0.46.0",
    "babel-jest": "*",
    "babel-preset-react-native": "1.9.1",
    "jest": "*",
    "fetch-mock": "^5.12.2"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "preset": "react-native",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "verbose": true,
    "collectCoverage": false,
    "moduleFileExtensions": [
      "js"
    ],
    "globals": {
      "__DEV__": true
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pilipa-cn/react-native-http.git"
  },
  "keywords": [
    "react-native",
    "alert"
  ],
  "author": "beansoft@126.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pilipa-cn/react-native-http/issues"
  },
  "homepage": "https://github.com/pilipa-cn/react-native-http#readme"
}